{
// The plugin looks for a .jshintrc file in the same directory as the source
// file you're prettifying (or any directory above if it doesn't exist, or in
// your home folder if everything else fails) and uses those options along
// the default ones.

// Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
// Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
// Documentation: http://www.jshint.com/docs/options/

"browser": true,
"esnext": true,
"quotmark": false,
"globals": {},
"predef": ["p5",
  "BLEND",
  "ADD",
  "BURN",
  "DARKEST",
  "DIFFERENCE",
  "DODGE",
  "EXCLUSION",
  "HARD_LIGHT",
  "LIGHTEST",
  "MULTIPLY",
  "OVERLAY",
  "REPLACE",
  "SCREEN",
  "SOFT_LIGHT",
  "arc",
  "atan2",
  "background",
  "beginShape",
  "blendMode",
  "CENTER",
  "color",
  "console",
  "constrain",
  "CORNER",
  "cos",
  "createCanvas",
  "createButton",
  "createInput",
  "dist",
  "ellipse",
  "endShape",
  "fill",
  "Firebase",
  "floor",
  "frameRate",
  "height",
  "key",
  "keyCode",
  "keyIsPressed",
  "LEFT",
  "lerpColor",
  "line",
  "loop",
  "mag",
  "map",
  "max",
  "millis",
  "min",
  "mouseButton",
  "mousePressed",
  "mouseReleased",
  "mouseX",
  "mouseY",
  "noFill",
  "noLoop",
  "noStroke",
  "PI",
  "pop",
  "push",
  "random",
  "rect",
  "rectMode",
  "RIGHT",
  "round",
  "setup",
  "SHIFT",
  "sin",
  "sqrt",
  "stroke",
  "strokeWeight",
  "text",
  "textAlign",
  "textAscent",
  "textSize",
  "textWidth",
  "touches",
  "touchX",
  "touchY",
  "TWO_PI",
  "unhex",
  "vertex",
  "width",
  "windowHeight",
  "windowWidth"
],
"globalstrict": true,
"latedef": false,
//set to true if you want to catch late def'd vars/fns
"strict": true,
"undef": true,
"unused": true
}
